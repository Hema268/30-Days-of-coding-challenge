#Day 2 Pattern printing
def printPat(n):
    #Code herefor 
    for i in range(n,0,-1):
        for j in range(n,0,-1):
            print((str(j)+" ")*i,end="")
        print("$",end="")

if __name__ == '__main__':
    t = int(input())
    for i in range(t):
        n = int(input())
        printPat(n)
        print()
